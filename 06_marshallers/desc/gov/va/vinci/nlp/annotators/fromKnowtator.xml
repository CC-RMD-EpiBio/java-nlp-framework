<?xml version="1.0" encoding="UTF-8"?><analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>true</primitive>
  <annotatorImplementationName>gov.va.vinci.nlp.framework.marshallers.knowtator.FromKnowtator</annotatorImplementationName>
  <analysisEngineMetaData>
    <name>fromKnowtator</name>
    <description>An annotator assumes the text was read in by a reader, annotations from a knowtator formatted file</description>
    <version>2012.01</version>
    <vendor/>
    <configurationParameters>
      <configurationParameter>
        <name>annotationInputDirectory</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>InputDir</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>Encoding</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>Language</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>knowtatorFileExtension</name>
        <description>The extension that each knowtator file has - .txt.knowtator.xml, or .knowtator.xml, or .xml</description>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>knowtatorLabelMap</name>
        <description>Pairs of knowtatorLabel|UIMA label</description>
        <type>String</type>
        <multiValued>true</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>fixOffsets</name>
        <description>One version of eHOST removes \r's from the \r\n, when it reads in files.  This causes an offset difference to CASs. Account for this difference when pulling in knowtator files created via this version of eHOST (when true).</description>
        <type>Boolean</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings>
      <nameValuePair>
        <name>Language</name>
        <value>
          <string>en</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>annotationInputDirectory</name>
        <value>
          <string>inputAndOutput/knowtatorFiles</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>InputDir</name>
        <value>
          <string>framework.data/inputAndOutput/input</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>knowtatorFileExtension</name>
        <value>
          <string>.xml</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>knowtatorLabelMap</name>
        <value>
          <array>
            <string>Symptom|gov.va.vinci.model.Symptom_Assigned</string>
          </array>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>fixOffsets</name>
        <value>
          <boolean>true</boolean>
        </value>
      </nameValuePair>
    </configurationParameterSettings>
    <typeSystemDescription>
      <imports>
        <import name="gov.va.vinci.Model"/>
        <import name="gov.va.vinci.flap.types.CSITypeDescriptor"/>
      </imports>
    </typeSystemDescription>
    <typePriorities/>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs/>
        <outputs/>
        <languagesSupported/>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>false</multipleDeploymentAllowed>
      <outputsNewCASes>true</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration/>
</analysisEngineDescription>
