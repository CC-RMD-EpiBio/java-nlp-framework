 */
public class ToJson extends JsonWriter {

  // =================================================
  /**
   * Constructor.
   *
   * @param pArgs the args
   * @throws ResourceInitializationException the resource initialization
   *           exception
   */
  // =================================================
  public ToJson(String[] pArgs) throws ResourceInitializationException {
    super(pArgs);

  } // end Constructor() -----------------------------

} // End Class ToJson() ------------------------------
